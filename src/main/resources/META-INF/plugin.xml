    <vendor  email="iicoming@hotmail.com" url="https://github.com/iicoming" />
    <description><![CDATA[
    project's git information and dependency information extraction for security analysis
    ]]></description>
    <project-components>
        <component>
            <implementation-class>com.thecastle.idea.component.OxpeckerProjectComponent</implementation-class>
        </component>
